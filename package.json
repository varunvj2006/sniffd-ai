{
  "name": "fragrance-finder",
  "version": "0.1.0",
  "private": true,
  "description": "Local fragrance finder using Ollama + Google Custom Search (Fragrantica), with a minimal modern black/white web UI",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --watch server.js server.js"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "ollama",
    "fragrance",
    "perfume",
    "fragrantica",
    "google-custom-search",
    "notes-extraction"
  ],
  "license": "MIT"
}
